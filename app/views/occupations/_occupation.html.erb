<div id="<%= dom_id occupation %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">User:</strong>
    <%= occupation.user_id %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Room:</strong>
    <%= occupation.room_id %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Day:</strong>
    <%= occupation.day %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Time s:</strong>
    <%= occupation.time_s %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Time e:</strong>
    <%= occupation.time_e %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Reservation:</strong>
    <%= occupation.reservation %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Confirmed:</strong>
    <%= occupation.confirmed %>
  </p>

</div>
